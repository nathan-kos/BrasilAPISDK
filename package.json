{
  "name": "@intway/brasilapi-sdk",
  "version": "0.1.2",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "brasilapi",
    "brazil",
    "sdk",
    "typescript",
    "api",
    "rest"
  ],
  "author": "Nathan Kosmalski",
  "license": "MIT",
  "description": "TypeScript SDK for consuming BrasilAPI endpoints with strong typing and error handling",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^22.15.30",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^51.0.1",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nathan-kos/BrasilAPISDK"
  },
  "bugs": {
    "url": "https://github.com/nathan-kos/BrasilAPISDK/issues"
  },
  "homepage": "https://github.com/nathan-kos/BrasilAPISDK#readme",
  "files": [
    "dist"
  ]
}
